{
  "name": "material-table",
  "version": "2.0.0",
  "description": "Datatable for React based on https://material-ui.com/api/table/ with additional features",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "babel": {
    "extends": "./configs/.babelrc"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./demo/webpack.config.js --mode development",
    "build": "babel src --out-dir dist",
    "lint": "npm run eslint && npm run tsc",
    "eslint": "eslint src/**",
    "tsc": "tsc --noEmit --lib es6,dom --skipLibCheck types/index.d.ts",
    "lint:fix": "eslint src/** --fix",
    "prettify": "prettier --write **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbrn/material-table.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "material",
    "datatable",
    "table"
  ],
  "author": "Mehmet Baran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbrn/material-table/issues"
  },
  "homepage": "https://github.com/mbrn/material-table#readme",
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@mui/lab": "5.0.0-alpha.120",
    "@mui/material": "5.11.10",
    "@mui/styles": "5.5.0",
    "babel-loader": "8.3.0",
    "babel-polyfill": "6.26.0",
    "buble": "0.20.0",
    "eslint": "8.35.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-standard": "5.0.0",
    "husky": "1.3.1",
    "prettier": "2.8.4",
    "pretty-quick": "2.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.1",
    "typescript": "4.9.5",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  },
  "dependencies": {
    "@date-io/date-fns": "2.16.0",
    "@emotion/styled": "11.10.6",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "debounce": "1.2.1",
    "fast-deep-equal": "2.0.1",
    "filefy": "0.1.11",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.5.28",
    "react-beautiful-dnd": "13.1.1",
    "react-double-scrollbar": "0.0.15"
  },
  "peerDependencies": {
    "@date-io/core": "^2.13.1",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@mui/styles": "5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
